# CCC S2 '17 'High Tide Low Tide'
# Sohom Saha
# Sept 12 2019


num1 = input()
tides = input()
array1 = []
for a in(tides.split(" ")):
  array1.append(int(a))
array1.sort()
low = array1[:len(array1)-len(array1)//2]
high = array1[(len(array1)-len(array1)//2):]
str1 = ""
bool1 = False
low.sort(reverse = True)
for i in range(0,len(low)):
  str1 += str(low[i])+" "
  if i > len(high)-1:
    print(str1)
    bool1 = True
  else:
    str1 += str(high[i])+" "
if not(bool1):
  print(str1)
